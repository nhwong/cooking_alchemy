
<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <% @ingredient_lists.each do |ingredient_list| %>
        <td><ul>
          <% ingredient_list.each do |ingredient| %>
          <li><%= ingredient %></li>
          <% end %>
        </ul><td>
        <% end %>
      </tr>
    </table>
  </div>
</div>

<div class="row">
  <div class="col-md-offset-1 col-md-12">
    <a href="/"  class="btn btn-primary">New Search</a>
    <a href="/summary/<%= @query.id %>"  class="btn btn-primary">Summarize</a>
  </div>
</div>
