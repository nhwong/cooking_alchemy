<h3> Query Summary: Most Common Words </h3>

<div class="row">
  <div class="col-md-offset-1 col-md-10">
    <table class="table">
      <tr>
        <% for i in 0..(@num_counts_to_display - 1) %>
        <th>Count: <%=@all_counts[i]%></th>
        <% end %>
      </tr>
      <% for i in 0..(@num_rows - 1)%>
      <tr>
        <% for j in 0..(@num_counts_to_display - 1) %>
        <td>
          <%=@display_arrays[@all_counts[j]][i]%>
        </td>
        <% end %>
      </tr>
      <% end %>
    </table>
  </div>
</div>
